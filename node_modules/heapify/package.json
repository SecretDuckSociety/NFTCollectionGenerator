{
  "name": "heapify",
  "version": "0.4.0",
  "description": "Blazing fast JavaScript priority queue",
  "main": "heapify.mjs",
  "scripts": {
    "test": "c8 --reporter=html --reporter=text mocha --recursive",
    "report-coverage": "c8 report --reporter=text-lcov > coverage.lcov && codecov",
    "count": "cloc heapify.mjs",
    "bench": "node benchmark/index.mjs",
    "linter": "eslint *.mjs test/**/*.mjs benchmark/**/*.mjs"
  },
  "keywords": [
    "priority queue",
    "queue",
    "heap",
    "minheap",
    "maxheap",
    "binary heap"
  ],
  "author": {
    "name": "Lucio Paiva",
    "url": "https://luciopaiva.com"
  },
  "homepage": "https://github.com/luciopaiva/heapify",
  "license": "MIT",
  "devDependencies": {
    "c8": "^7.3.0",
    "cloc": "^2.5.1",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "fastpriorityqueue": "0.6.3",
    "flatqueue": "1.1.0",
    "google-closure-library": "20200830.0.0",
    "mocha": "^7.1.1",
    "tinyqueue": "2.0.3"
  },
  "dependencies": {}
}
